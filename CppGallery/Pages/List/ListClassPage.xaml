                <local:FunctionExpander Folder="list__list/rbegin" />
                <local:FunctionExpander Folder="list__list/remove" />
                <local:FunctionExpander Folder="list__list/remove_if" />
                <local:FunctionExpander Folder="list__list/rend" />
                <local:FunctionExpander Folder="list__list/resize" />
                <local:FunctionExpander Folder="list__list/reverse" />
                <local:FunctionExpander Folder="list__list/size" />
                <local:FunctionExpander Folder="list__list/sort" />
                
                <local:CustomExpander IsEnabled="False">
                    <Expander.Header>
                        <local:EHeader Title="splice() 関数" Sentence="aaa"/>
                    </Expander.Header>
                </local:CustomExpander>

                <local:FunctionExpander Folder="list__list/swap" />
                <local:FunctionExpander Folder="list__list/unique" />
            </local:BlockPanel>

            <local:LinkButtons MSURL="https://learn.microsoft.com/ja-jp/cpp/standard-library/list-class?view=msvc-170" CppJPURL="https://cpprefjp.github.io/reference/list/list.html" />

        </local:OuterPanel>

    </ScrollViewer>
