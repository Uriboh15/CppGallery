                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/LightSkyBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/LightSlateGray" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/LightSteelBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
