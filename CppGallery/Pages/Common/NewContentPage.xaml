                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Bisque" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Black" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/BlanchedAlmond" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Blue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/BlueViolet" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Brown" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/BurlyWood" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/CadetBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Chartreuse" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Chocolate" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Coral" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/CornflowerBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Cornsilk" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Crimson" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Cyan" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkCyan" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkGoldenrod" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkGray" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkGreen" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkKhaki" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkMagenta" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkOliveGreen" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkOrange" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkOrchid" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkRed" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkSalmon" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkSeaGreen" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkSlateBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkSlateGray" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkTurquoise" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DarkViolet" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DeepPink" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DeepSkyBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DimGray" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/DodgerBlue" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Firebrick" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/FloralWhite" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/ForestGreen" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Fuchsia" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Gainsboro" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/GhostWhite" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Gold" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/Goldenrod" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                </local:BlockPanel>

                <local:BlockPanel Header="ContentPropertyAttribute" TargetMinCppVersion="Cpp17" >
                    <local:FunctionExpander Folder="winrt/Windows.UI.Xaml.Markup.ContentPropertyAttribute/ContentPropertyAttribute" SampleType="None" CodeLanguage="MIDL" />
                    <local:FunctionExpander Folder="winrt/Windows.UI.Xaml.Markup.ContentPropertyAttribute/Name" SampleType="None" CodeLanguage="MIDL" Icon="Object" />
                </local:BlockPanel>

                <local:BlockPanel Header="FileOpenPicker" TargetMinCppVersion="Cpp17" >
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/FileOpenPicker" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/CommitButtonText" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/SettingsIdentifier" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/SuggestedStartLocation" SampleType="None" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/ViewMode" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/PickMultipleFilesAsync" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FileOpenPicker/PickSingleFileAsync" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                </local:BlockPanel>

                <local:BlockPanel Header="FolderPicker" TargetMinCppVersion="Cpp17" >
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/FolderPicker" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/CommitButtonText" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/SettingsIdentifier" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/SuggestedStartLocation" SampleType="None" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/ViewMode" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.Storage.Pickers.FolderPicker/PickSingleFolderAsync" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                </local:BlockPanel>

                <local:BlockPanel Header="XamlReader" TargetMinCppVersion="Cpp17" >
                    <local:FunctionExpander Folder="winrt/Windows.UI.Xaml.Markup.XamlReader/Load" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                </local:BlockPanel>
            </local:OuterPanel>
        </ScrollViewer>
    </Grid>
</Page>
