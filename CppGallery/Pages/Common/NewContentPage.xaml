                    <local:ToRightButton Style="{StaticResource WinRTWindowsUIXamlDocuments}" />
                </local:BlockPanel>

                <local:SymbolBlockPanel Symbol="Class" >
                    <local:ToRightButton Style="{StaticResource WinRTWindowsGlobalizationJapanesePhoneticAnalyzer}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemPowerPowerManager}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsUIXamlDocumentsInline}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsUIXamlDocumentsLineBreak}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsUIXamlDocumentsRun}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsUIXamlDocumentsTextElement}" />
                </local:SymbolBlockPanel>

                <local:SymbolBlockPanel Symbol="Enum" >
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsCollectionChange}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemPowerBatteryStatus}" />
