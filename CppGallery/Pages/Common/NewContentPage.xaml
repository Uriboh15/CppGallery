                    <local:FunctionExpander Folder="winrt/Windows.System.Inventory.InstalledDesktopApp/DisplayVersion" SampleType="GUIApp" CodeLanguage="CppWinRT" />
                    <local:FunctionExpander Folder="winrt/Windows.System.Inventory.InstalledDesktopApp/Id" SampleType="GUIApp" CodeLanguage="CppWinRT" />
