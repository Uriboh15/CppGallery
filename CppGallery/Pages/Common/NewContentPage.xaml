                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsMapChangedEventHandler}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsVectorChangedEventHandler}" />
                </local:SymbolBlockPanel>

                <local:BlockPanel Header="Colors" TargetMinCppVersion="Cpp17" >
                    <local:FunctionExpander Folder="winrt/Windows.UI.Colors/MintCream" SampleType="GUIApp" CodeLanguage="CppWinRT" />
