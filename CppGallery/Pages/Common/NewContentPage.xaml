                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsIKeyValuePair}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsIMap}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsIMapView}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsIVector}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollectionsIVectorView}" />
                </local:SymbolBlockPanel>

