                <local:BlockPanel Header="ヘッダー" >
                    <local:ToRightButton Style="{StaticResource WinRTWindowsFoundationCollections}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemDisplay}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemInventory}" />
                </local:BlockPanel>

                <local:SymbolBlockPanel Symbol="Class" >
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemDisplayDisplayRequest}" />
                    <local:ToRightButton Style="{StaticResource WinRTWindowsSystemInventoryInstalledDesktopApp}" />
                </local:SymbolBlockPanel>

                <local:SymbolBlockPanel Symbol="Enum" >
                    
                </local:SymbolBlockPanel>

