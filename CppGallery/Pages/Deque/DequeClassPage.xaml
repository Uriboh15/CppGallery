                <local:FunctionExpander Folder="deque__deque/size" />
                <local:FunctionExpander Folder="deque__deque/swap" />

            </local:BlockPanel>

            <local:LinkButtons MSURL="https://learn.microsoft.com/ja-jp/cpp/standard-library/deque-class?view=msvc-170" CppJPURL="https://cpprefjp.github.io/reference/deque/deque.html" />
        </local:OuterPanel>
