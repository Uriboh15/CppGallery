                    <local:EnumPanel>
                        <local:ItemGrid Title="long long quot" Sentence="商"/>
                        <local:ItemGrid Title="long long rem" Sentence="余り"/>
                    </local:EnumPanel>
                </local:CustomExpander>
            </local:SymbolBlockPanel>


