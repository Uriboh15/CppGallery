                    <local:EnumPanel>
                        <local:ItemGrid Title="long quot" Sentence="商"/>
                        <local:ItemGrid Title="long rem" Sentence="余り"/>
                    </local:EnumPanel>
                </local:CustomExpander>

                <local:CustomExpander>
