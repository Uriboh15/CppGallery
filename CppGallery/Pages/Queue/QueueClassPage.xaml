                <local:ItemGrid Title="value_type" Sentence="queue の要素の型" />
            </local:SymbolBlockPanel>

            <local:SymbolBlockPanel Symbol="Method" >
                <local:FunctionExpander Folder="queue__queue/back" />
                <local:FunctionExpander Folder="queue__queue/emplace" />
                <local:FunctionExpander Folder="queue__queue/empty" />
                <local:FunctionExpander Folder="queue__queue/front" />
                <local:FunctionExpander Folder="queue__queue/pop" />
                <local:FunctionExpander Folder="queue__queue/push" />
                <local:FunctionExpander Folder="queue__queue/size" />
            </local:SymbolBlockPanel>

            <local:LinkButtons MSURL="https://learn.microsoft.com/ja-jp/cpp/standard-library/queue-class?view=msvc-170" CppJPURL="https://cpprefjp.github.io/reference/queue/queue.html" />

